            function SlideShow (img,delay,order,type,el,size){
                if (arguments.length>0) this.init(img,delay,order,type,el,size);
            }//Class Constructor

            SlideShow.prototype.init = function(img,delay,order,type,el,size) {
                /* img=[{src:"...", description:"..."},...], delay=1000-10000(ms), el=document.querySelector('...'), size={width:"1%-100%", height:"100-1920"}*/
                if (delay <= 10000 && delay >= 1000) {
                    this.delay = delay;
                } else {
                    this.delay = 4000;
                }/*Delay may be >=1sec and <=10sec. Otherwise used default delay.*/
                if (order == "bidi") this.order=order;
                else if (order == "rtl") this.order=order;
                else if (order == "ltr") this.order=order;
                else if (order == "rdm") this.order=order;
                else this.order="bidi";
                if (type == "crossfade") this.type=type;
                else if (type == "slide") this.type=type;
                else this.type="slide";
                if (el instanceof  Element) {
                    this.el = el;
                } else {
                    this.el = document.body.querySelector('.container');
                }
                var minSize=100;    /*in px*/
                var maxSize=1920;   /*in px*/
                var height=window.getComputedStyle(this.el,null).getPropertyValue("height").slice(0,-2);
                var width=window.getComputedStyle(this.el,null).getPropertyValue("width").slice(0,-2);
                if ((size) && (size.width) && size.width.slice(-1)=="%" && size.width.slice(0,-1)<=100 && size.width.slice(0,-1)>0 && (width * size.width.slice(0, -1) / 100) >= minSize){
                    this.width = ""+ width * size.width.slice(0, -1) / 100 + "px";
                }else if ((size) && (size.width) && size.width <= maxSize && size.width >= minSize && size.width <= width) {
                    this.width = ""+size.width+"px";
                }else if (((!size) || ((size) && (!size.width))) && width <= maxSize && width >= minSize) {
                    this.width = ""+width+"px";
                }else if ((size) && (size.width) && (size.width > maxSize || size.width < minSize) && minSize <= width) {
                    this.width = ""+minSize+"px";
                }else {
                    this.width = "0px"; //minSize > element.width - so we set width to 0px
                }
                if ((size) && (size.height) && size.height.slice(-1)=="%" && size.height.slice(0,-1)<=minSize && size.height.slice(0,-1)>0 && (height * size.height.slice(0, -1) / 100) >= minSize) {
                    this.height = "" + height * size.height.slice(0, -1) / 100 + "px";
                }else if ((size) && (size.height) && size.height <= maxSize && size.height >= minSize && size.height <= height) {
                    this.height = ""+size.height+"px";
                }else if (((!size) || ((size) && (!size.height))) && height <= maxSize && height >= minSize) {
                    this.height = "" + height + "px";
                }else if ((size) && (size.height) && (size.height > maxSize || size.height < minSize) && minSize <= height) {
                    this.height = ""+minSize+"px";
                }else {
                    this.height = "0px"; //minSize > element.height - so we set height to 0px
                }
                this.renderWrap();
                this.animationInProgress=false;
                var self = this;
                if (img) {
                    this.img = img;
                    this.renderImages();
                } else {
                    this.timedGetJSON('/images.ajax', 5000, handleRequest);
                    function handleRequest(req) {
                        if (req.data && req.data.length>0){self.img = req.data}
                        else {self.img =[{"src":"/images/iron man.jpg", "description":"IRON MAN"},{"src":"/images/captain america.jpg", "description":"CAPTAIN AMERICA"},{"src":"/images/hulk.jpeg", "description":"HULK"},{"src":"/images/thor.jpg", "description":"THOR"},{"src":"/images/black widow.jpg", "description":"BLACK WIDOW"},{"src":"/images/hawkeye.jpg", "description":"HAWKEYE"},{"src":"/images/loki.jpg", "description":"LOKI"}]}
                        self.renderImages();
                    }
                }
            }//Initialization

            SlideShow.prototype.renderWrap = function() {
                this.wrap = document.createElement("div");
                this.wrap.className = "slideshow-wrap";
                this.wrap.innerHTML += '<i class="fa fa-spinner fa-pulse" style="text-shadow:none; font-size:100px; position:relative; top:50%; margin-top:-50px"></i>';
                this.el.appendChild(this.wrap);
                this.wrap.style.width = this.width;
                this.wrap.style.height = this.height;
            }//Render container for images

            SlideShow.prototype.renderImages = function() {
                this.wrap.innerHTML='';
                this.inner = document.createElement("div");
                this.inner.className = "slideshow-inner";
                this.inner.classList.add(this.type);
                var x=20; //button start margin values & 13 is buttons size
                for (var len=this.img.length; (x*len+(x-13))>this.width.slice(0, -2); x--){};
                for(var i = 0, len=this.img.length; i < len; i++) {
                    if (len > 1) this.wrap.innerHTML +=('<span class="buttons" data-button="'+(i+1)+'" data-active="'+0+'" style="margin-left:'+(+(x-14)/2-x*len/2+x*i)+'px;"></span> \n');
                    if (this.img[i].description) {
                        this.inner.innerHTML +=('<div class="item"> \n <img src="'+this.img[i].src+'" /> \n<div class="description"> \n<h2>'+this.img[i].description+'</h2></div></div>');
                    } else{
                        this.inner.innerHTML +=('<div class="item"> \n <img src="'+this.img[i].src+'" /> </div>');
                    }
                }
                this.wrap.innerHTML +=('<span class="fa fa-arrow-circle-left fa-2x arrows" data-arrow="left"></span> \n');
                this.wrap.innerHTML +=('<span class="fa fa-arrow-circle-right fa-2x arrows " data-arrow="right"></span> \n');
                this.wrap.appendChild(this.inner);
                this.items = this.wrap.querySelectorAll('div img, div  div.description, .arrows, .buttons');
                this.btn = this.wrap.querySelectorAll('.buttons');
                this.divs = this.inner.querySelectorAll('.item');
                this.arrowLeft=this.wrap.querySelector('.arrows[data-arrow="left"]');
                this.arrowRight=this.wrap.querySelector('.arrows[data-arrow="right"]');
                if (this.order == "rtl" && this.img.length>1) {
                    this.btn[this.btn.length-1].dataset.active = "1";
                    this.inner.dataset.status = this.btn.length-1;
                    this.divs[this.btn.length-1].className="item active";
                } else if (this.img.length>1){
                    this.btn[0].dataset.active = "1";
                    this.inner.dataset.status = 0;
                    this.divs[0].className="item active";
                }
                this.direction = 1; //for bi-directional order
                this.bindEvents();
                if (this.state=="started" && !(this.interval)) this.start();
            }//Render images

            SlideShow.prototype.bindEvents = function(){
                var self=this;
                (function() {
                    for(var i = 0, len = self.items.length; i < len; i++) {
                        (function() {
                                self.items[i].addEventListener("mouseover", function () {clearInterval(self.interval)}, true);
                                self.items[i].addEventListener("mouseout", function () {
                                    if (self.state=="started") self.start();
                            });
                        })();
                    }
                    self.arrowLeft.addEventListener("click", function() {
                        if (self.animationInProgress==false) {
                            if (self.inner.dataset.status == 0) self.inner.dataset.status = self.img.length - 1;
                            else self.inner.dataset.status--;
                            self.direction = -1;
                        }
                    });
                    self.arrowRight.addEventListener("click", function() {
                        if (self.animationInProgress==false) {
                            if (self.inner.dataset.status == self.img.length - 1) self.inner.dataset.status = 0;
                            else self.inner.dataset.status++;
                            self.direction = 1;
                        }
                    });
                    for(var i = 0, len = self.btn.length; i < len; i++) {
                        (function () {
                            var j = i;
                            self.btn[j].addEventListener("click", function () {
                                if (self.animationInProgress==false) {
                                    self.btn[j].dataset.active = "1";
                                    self.inner.dataset.status = j;
                                    if (self.order=="bidi" && j==self.btn.length-1) self.direction = -1;
                                    if (self.order=="bidi" && j==0) self.direction = 1;
                                }
                            });
                            self.divs[j].addEventListener("transitionend", function () {
                                if (!self.divs[self.inner.dataset.status].classList.contains("active")){
                                    self.divs[self.inner.dataset.status].className = "item active";
                                    for(var i= 0; i < len;i++){
                                        if (i==self.inner.dataset.status){continue};
                                        self.divs[i].className = "item";
                                    }
                                    setTimeout(function(){self.animationInProgress = false;},0);
                                }
                            });
                        })();
                    }
                })();
                var mutationObserver = new MutationObserver(mutationObjectCallback);
                function mutationObjectCallback(mutationRecordsList) {
                    mutationRecordsList.forEach(function(mutationRecord) {
                        if ("attributes" === mutationRecord.type && mutationRecord.oldValue !== self.inner.dataset.status) {
                            self.animationInProgress = true;
                            var direction, order;
                            if (mutationRecord.oldValue == self.img.length - 1 && self.inner.dataset.status == 0 && self.btn[self.inner.dataset.status].dataset.active == "0" && self.direction == 1) {
                                order = "next";
                                direction = "left";
                            } else if (mutationRecord.oldValue == 0 && self.inner.dataset.status == self.img.length - 1 && self.btn[self.inner.dataset.status].dataset.active == "0" && self.direction == -1) {
                                order = "prev";
                                direction = "right";
                            } else if (mutationRecord.oldValue > self.inner.dataset.status) {
                                order = "prev";
                                direction = "right";
                            } else if (mutationRecord.oldValue < self.inner.dataset.status) {
                                order = "next";
                                direction = "left";
                            }
                            self.divs[self.inner.dataset.status].classList.add(order);
                            self.divs[self.inner.dataset.status].offsetWidth;// force reflow
                            self.divs[mutationRecord.oldValue].classList.add(direction);
                            self.divs[self.inner.dataset.status].classList.add(direction);
                            self.btn[self.inner.dataset.status].dataset.active = "1";
                            for (var i = 0, len = self.btn.length; i < len; i++) {
                                if (i != self.inner.dataset.status) {self.btn[i].dataset.active = "0"}
                            }
                        }
                    });
                }
                mutationObserver.observe(self.inner, {attributes: true, attributeFilter: ["data-status"], attributeOldValue: true, childList: false});
            }//Binding events

            SlideShow.prototype.stop = function() {
                clearInterval(this.interval);
                this.state="stopped";
            }//Stop continual animation

            SlideShow.prototype.start = function() {
                this.state="started";
                if (!this.direction) {
                    return;
                } // Don't start animation before render complete
                var self=this;
                this.interval=setInterval(function() { self.animate.call(self) }, self.delay);
            }//Start continual animation

            SlideShow.prototype.animate = function() {
                if (this.img.length<=1) return;
                if (this.animationInProgress==true) return;
                if (this.order=="bidi") {
                    if (this.inner.dataset.status==this.img.length-1) {
                        this.inner.dataset.status--;
                        this.direction=-1;
                    } else if (this.inner.dataset.status==0) {
                        this.inner.dataset.status++;
                        this.direction=1;
                    } else if (this.direction==1) this.inner.dataset.status++;
                    else if (this.direction==-1) this.inner.dataset.status--;
                }
                if (this.order=="rtl") {
                    if (this.inner.dataset.status==0) this.inner.dataset.status=this.img.length-1;
                    else this.inner.dataset.status--;
                    this.direction=-1;
                }
                if (this.order=="ltr") {
                    if (this.inner.dataset.status==this.img.length-1) this.inner.dataset.status=0;
                    else this.inner.dataset.status++;
                    this.direction=1;
                }
                if (this.order=="rdm") this.inner.dataset.status=Math.floor(Math.random()*this.img.length);
            }//Next image

            SlideShow.prototype.timedGetJSON = function(url, timeout, callback) {
                var request = new XMLHttpRequest(); // Create new request.
                var timedout = false; // Whether we timed out or not.
                // Start a timer that will abort the request after timeout ms.
                var timer = setTimeout(function() { // Start a timer. If triggered,
                    timedout = true; // set a flag and then
                    request.abort(); // abort the request.
                        },timeout);
                request.open("GET", url, true); // Specify URL to fetch
                request.onreadystatechange = function () { // Define event listener.
                    if (request.readyState !== 4) return; // Ignore incomplete requests.
                    if (timedout) callback(""); // Ignore aborted requests.
                    clearTimeout(timer); // Cancel pending timeout.
                    if (request.status === 200) { // If request was successful
                        callback(JSON.parse(request.responseText)); // pass response to callback.
                    }
                };
                request.send(null); // Send the request now
            }//Send request to the server

