                if (delay <= 10000 && delay >= 1000) {
                    this.delay = delay;
                } else {
                    this.delay = 4000;
                }/*Delay may be >=1sec and <=10sec. Otherwise used default delay.*/
                if (type="rtl") this.type=type;
                    else if (type="ltr") this.type=type;
                    else if (type="bidi") this.type=type;
                    else if (type="rand") this.type=type;
                    else this.type="bidi";
                if (el instanceof  Element) {
                    this.el=el;
                } else {
                    this.el=document.body;
                }
                this.render();
                this.slides = this.wrap.querySelectorAll('.slideshow-inner img, .slideshow-inner div');
                this.btn = this.wrap.querySelectorAll('.buttons');
                this.direction = 1;
                this.btn[0].dataset.active="1";
                this.bindClick();
                this.bindPause();
                this.start();
            }/*Slideshow Class Constructor Function*/

            SlideShow.prototype.render = function() {
                this.wrap = document.createElement("div");
                this.wrap.className = "slideshow-wrap";
                this.inner = document.createElement("div");
                this.inner.className = "slideshow-inner";
                this.inner.ul = document.createElement("ul");
                this.style = document.createElement('style');
                this.style.type = 'text/css';
                this.styleText = ".slideshow-inner>ul {width: "+100*this.img.length+"%} .slideshow-wrap .buttons[data-button='"+this.img.length+"'][data-active='1']~.arrows[data-arrow='right']{display: none;}";
                for(var i = 0, len=this.img.length; i < len; i++) {
                    this.wrap.innerHTML +=('<span class="buttons" data-button="'+(i+1)+'" data-active="'+0+'"></span> \n');
                    if (this.img[i].description) {
                        this.inner.ul.innerHTML +=('<li name="slide'+(i+1)+'"> \n <img src="'+this.img[i].src+'" /> \n<div class="description"> \n<h2>'+this.img[i].description+'</h2></div></li>');
                    }
                    else{
                        this.inner.ul.innerHTML +=('<li name="slide'+(i+1)+'"> \n <img src="'+this.img[i].src+'" /> \n');
                    }
                    this.styleText += ".slideshow-wrap .buttons[data-button='"+(i+1)+"'][data-active='1']~.slideshow-inner>ul { left: -"+100*i+"%;}\
                    .slideshow-wrap .buttons[data-button='"+(i+1)+"'] { margin-left: "+(-18*len/2+18*i)+"px;}";
                }
                this.wrap.innerHTML +=('<span class="arrows" data-arrow="left"><</span> \n');
                this.wrap.innerHTML +=('<span class="arrows" data-arrow="right">></span> \n');
                this.inner.appendChild(this.inner.ul);
                this.wrap.appendChild(this.inner);
                this.el.appendChild(this.wrap);

                if (this.style.styleSheet) {
                    this.style.styleSheet.cssText = this.styleText;
                } else {
                    if (this.style.innerText == '') {
                        this.style.innerText = this.styleText;
                    } else {
                        this.style.innerHTML = this.styleText;
                    }
                }
                document.head.appendChild(this.style);

            }/*Slideshow Render Function*/

            SlideShow.prototype.animate = function() {
                if (this.img.length<=1) return;
                var i = this.wrap.querySelector('.buttons[data-active="1"]').dataset.button-1;
                if (i+this.direction<0) this.direction=1;
                if (i+this.direction>this.btn.length-1) this.direction=-1;
                this.btn[i].dataset.active="0";
                this.btn[i+this.direction].dataset.active="1";
            }/*Slideshow one-step animation Function*/

            SlideShow.prototype.start =  function() {
                var self=this;
                this.interval=setInterval(function() { self.animate.call(self) }, self.delay);
            }/*Slideshow start continual animation Function*/

            SlideShow.prototype.stop = function() {
                clearInterval(this.interval);
            }/*Slideshow stop continual animation Function*/

            SlideShow.prototype.bindClick = function(){
                var self=this;
                self.arrowLeft=this.wrap.querySelector('.arrows[data-arrow="left"]');
                self.arrowRight=this.wrap.querySelector('.arrows[data-arrow="right"]');
                (function() {
                    self.arrowLeft.addEventListener("click", function() {
                        self.stop();
                        var i = self.wrap.querySelector('.buttons[data-active="1"]').dataset.button-1;
                        self.btn[i].dataset.active="0";
                        self.btn[--i].dataset.active="1";
                        self.direction=-1;
                        self.start();
                    });
                    self.arrowRight.addEventListener("click", function() {
                        self.stop();
                        var i = self.wrap.querySelector('.buttons[data-active="1"]').dataset.button-1;
                        self.btn[i].dataset.active="0";
                        self.btn[++i].dataset.active="1";
                        self.direction=1;
                        self.start();
                    });
                    for(var i = 0, len = self.btn.length; i < len; i++) {
                        (function () {
                            var j = i;
                            self.btn[j].addEventListener("click", function () {
                                self.stop();
                                self.btn[self.wrap.querySelector('.buttons[data-active="1"]').dataset.button-1].dataset.active="0";
                                self.btn[j].dataset.active="1";
                                if (j===1) self.direction=1;
                                if (j===self.btn.length-1) self.direction=-1;
                                self.start();
                            });
                        })();
                    }
                })();
            }/*Slideshow click EventListener Function*/

            SlideShow.prototype.bindPause = function() {
                var self=this;
                for(var i = 0, len = self.slides.length; i < len; i++) {
                    (function() {
                        self.slides[i].addEventListener("mouseover", function() {
                            self.stop();
                        });
                        self.slides[i].addEventListener("mouseout", function() {
                            self.start();
                        });
                    })();
                }
            }/*Slideshow Pause on hover EventListener Function*/

