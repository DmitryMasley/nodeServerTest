        window.onload = function() {

            var SlideShow =function(el,img,desc){
                if (!img) {
                    this.img=["http://7themes.su/_ph/40/806761392.jpg","http://7themes.su/_ph/40/109111701.jpg","http://7themes.su/_ph/38/351815896.jpg","http://7themes.su/_ph/23/855840339.jpg","http://7themes.su/_ph/38/686375918.jpg"];
                    this.desc=["The Hobbit","Мстители: Эра Альтрона","Инь и ян","Lamborghini aventador","Отражение в очках"];
                }/*If no input array with images - take pre-defined array with images and pre-defined array with their descriptions*/
                else {
                    this.img=img;
                    if (desc) this.desc=desc;
                }/*If is input array with images - take that array. If is input array with images descriptions - take it too*/
                if(el instanceof  Element) {
                    this.el=el;
                } else {
                    this.el=document.body;
                }
                this.render();
                this.slides = this.el.querySelectorAll('[class="slideshow-inner"] img, [class="slideshow-inner"] div');
                this.btn = this.el.querySelectorAll('[class="slideshow-wrap"] [class="buttons"]');
                this.direction = 1;
                this.btn[0].dataset.active="1";
                this.bindClick();
                this.bindPause();
                this.start();
            }/*Slideshow Class Constructor Function*/
            SlideShow.prototype.render = function() {
                this.wrap = document.createElement("div");
                this.wrap.className = "slideshow-wrap";
                this.inner = document.createElement("div");
                this.inner.className = "slideshow-inner";
                this.inner.ul = document.createElement("ul");
                for(var i = 0, len=this.img.length; i < len; i++) {
                    this.wrap.innerHTML +=('<span class="buttons" data-button="'+(i+1)+'" data-active="'+0+'"></span> \n');
                    if (this.desc) {
                        this.inner.ul.innerHTML +=('<li name="slide'+(i+1)+'"> \n <img src="'+this.img[i]+'" /> \n<div class="description"> \n<h2>'+this.desc[i]+'</h2></div></li>');
                    }
                    else{
                        this.inner.ul.innerHTML +=('<li name="slide'+(i+1)+'"> \n <img src="'+this.img[i]+'" /> \n');
                    }
               }
                this.wrap.innerHTML +=('<span class="arrows" data-arrow="left"><</span> \n');
                this.wrap.innerHTML +=('<span class="arrows" data-arrow="right">></span> \n');
                this.inner.appendChild(this.inner.ul);
                this.wrap.appendChild(this.inner);
                this.el.appendChild(this.wrap);
            }/*Slideshow Render Function*/
            SlideShow.prototype.animate = function() {
                var i = this.el.querySelectorAll('[class="slideshow-wrap"] [class="buttons"][data-active="1"]')[0].dataset.button-1;
                if (i+this.direction<0) this.direction=1;
                if (i+this.direction>this.btn.length-1) this.direction=-1;
                this.btn[i].dataset.active="0";
                this.btn[i+this.direction].dataset.active="1";
            }/*Slideshow one-step animation Function*/
            SlideShow.prototype.start =  function() {
                var self=this;
                this.interval=setInterval(function() { self.animate.call(self) }, 4000);
            }/*Slideshow start continual animation Function*/
            SlideShow.prototype.stop = function() {
                clearInterval(this.interval);
            }/*Slideshow stop continual animation Function*/
            SlideShow.prototype.bindClick = function(){
                var self=this;
                self.arrowLeft=this.el.querySelectorAll('[class="slideshow-wrap"] [class="arrows"][data-arrow="left"]')[0];
                self.arrowRight=this.el.querySelectorAll('[class="slideshow-wrap"] [class="arrows"][data-arrow="right"]')[0];
                (function() {
                    self.arrowLeft.addEventListener("click", function() {
                        self.stop();
                        var i = self.el.querySelectorAll('[class="slideshow-wrap"] [class="buttons"][data-active="1"]')[0].dataset.button-1;
                        self.btn[i].dataset.active="0";
                        self.btn[--i].dataset.active="1";
                        self.direction=-1;
                        self.start();
                    });
                    self.arrowRight.addEventListener("click", function() {
                        self.stop();
                        var i = self.el.querySelectorAll('[class="slideshow-wrap"] [class="buttons"][data-active="1"]')[0].dataset.button-1;
                        self.btn[i].dataset.active="0";
                        self.btn[++i].dataset.active="1";
                        self.direction=1;
                        self.start();
                    });
                    for(var i = 0; i < self.btn.length; i++) {
                        (function () {
                            var j = i;
                            self.btn[j].addEventListener("click", function () {
                                self.stop();
                                self.btn[self.el.querySelectorAll('[class="slideshow-wrap"] [class="buttons"][data-active="1"]')[0].dataset.button-1].dataset.active="0";
                                self.btn[j].dataset.active="1";
                                if (j==1) self.direction=1;
                                if (j==self.btn.length-1) self.direction=-1;
                                self.start();
                            });
                        })();
                    }
                })();
            }/*Slideshow click EventListener Function*/
            SlideShow.prototype.bindPause = function() {
                var self=this;
                for(var i = 0; i < self.slides.length; i++) {
                    (function() {
                        self.slides[i].addEventListener("mouseover", function() {
                            self.stop();
                        });
                        self.slides[i].addEventListener("mouseout", function() {
                            self.start();
                        });
                    })();
                }
            }/*Slideshow Pause on hover EventListener Function*/
            var qwe = new SlideShow(document.querySelectorAll('[class="first"]')[0]);
