        window.onload = function() {

            function SlideShow (img,delay,el,size){
                if (arguments.length>0) this.render(img,delay,el,size);
            }/*Slideshow Class Constructor Function*/

            SlideShow.prototype.render = function(img,delay,el,size) {
                this.init(img,delay,el,size);
                this.wrap = document.createElement("div");
                this.wrap.className = "slideshow-wrap";
                this.inner = document.createElement("div");
                this.inner.className = "slideshow-inner";
                this.inner.ul = document.createElement("ul");
                for(var i = 0, len=this.img.length; i < len; i++) {
                    this.wrap.innerHTML +=('<span class="buttons" data-button="'+(i+1)+'" data-active="'+0+'" style="margin-left:'+(-18*len/2+18*i)+'px;"></span> \n');
                    if (this.img[i].description) {
                        this.inner.ul.innerHTML +=('<li name="slide'+(i+1)+'" style="width:'+100/this.img.length+'%; "> \n <img src="'+this.img[i].src+'" /> \n<div class="description"> \n<h2>'+this.img[i].description+'</h2></div></li>');
                    }
                    else{
                        this.inner.ul.innerHTML +=('<li name="slide'+(i+1)+'" style="width:'+100/this.img.length+'%; "> \n <img src="'+this.img[i].src+'" /> \n');
                    }
                }
                this.wrap.innerHTML +=('<span class="arrows" data-arrow="left"><</span> \n');
                this.wrap.innerHTML +=('<span class="arrows" data-arrow="right">></span> \n');
                this.inner.appendChild(this.inner.ul);
                this.wrap.appendChild(this.inner);
                this.el.appendChild(this.wrap);
                this.wrap.style.width=this.width;
                this.wrap.style.height=this.height;
                this.inner.ul.style.width=''+100*this.img.length+'%';
                this.inner.ul.style.height=this.height;
                this.inner.ul.style.left="0%";
                this.slides = this.wrap.querySelectorAll('.slideshow-inner img, .slideshow-inner div');
                this.btn = this.wrap.querySelectorAll('.buttons');
                this.arrowLeft=this.wrap.querySelector('.arrows[data-arrow="left"]');
                this.arrowRight=this.wrap.querySelector('.arrows[data-arrow="right"]');
                this.arrowLeft.style.display="none";
                this.btn[0].dataset.status="1";
                this.inner.dataset.status=0;
                this.bindEvents();
            }/*render*/

            SlideShow.prototype.init = function(img,delay,el,size) {
                /* img=[{src:"...", description:"..."},...], delay=1000-10000(ms), el=document.querySelector('...'), size={width:"1%-100%", height:"100-1920"}*/
                this.img = img || [{src:"http://7themes.su/_ph/40/806761392.jpg", description:"The Hobbit"},{src:"http://7themes.su/_ph/40/109111701.jpg", description:"Мстители: Эра Альтрона"},{src:"http://7themes.su/_ph/38/351815896.jpg", description:"Инь и ян"},{src:"http://7themes.su/_ph/23/855840339.jpg", description:"Lamborghini aventador"},{src:"http://7themes.su/_ph/38/686375918.jpg", description:"Отражение в очках"}];
                if (delay <= 10000 && delay >= 1000) {
                    this.delay = delay;
                } else {
                    this.delay = 4000;
                }/*Delay may be >=1sec and <=10sec. Otherwise used default delay.*/
                if (el instanceof  Element) {
                    this.el = el;
                } else {
                    this.el = document.body;
                }
                var minSize=100;    /*in px*/
                var maxSize=1920;   /*in px*/
                var height=window.getComputedStyle(this.el,null).getPropertyValue("height").slice(0,-2);
                var width=window.getComputedStyle(this.el,null).getPropertyValue("width").slice(0,-2);
                if ((size) && (size.width) && size.width.slice(-1)=="%" && size.width.slice(0,-1)<=100 && size.width.slice(0,-1)>0){
                    this.width = ""+width*size.width.slice(0,-1)/100+"px";
                }
                else if ((size) && (size.width) && size.width <= maxSize && size.width >= minSize && size.width <= width) {
                    this.width = ""+size.width+"px";
                }
                else if (((!size) || ((size) && (!size.width))) && width <= maxSize && width >= minSize) {
                    this.width = ""+width+"px";
                }
                else {
                    this.width = "180px";
                }
                if ((size) && (size.height) && size.height.slice(-1)=="%" && size.height.slice(0,-1)<=minSize && size.height.slice(0,-1)>0) {
                    this.height = "" + height * size.height.slice(0, -1) / 100 + "px";
                }
                else if ((size) && (size.height) && size.height <= maxSize && size.height >= minSize && size.height <= height) {
                    this.height = ""+size.height+"px";
                }
                else if (((!size) || ((size) && (!size.height))) && height <= maxSize && height >= minSize) {
                    this.height = ""+height+"px";
                }
                else {
                    this.height = "110px";
                }
            }/*initialization*/

            SlideShow.prototype.bindEvents = function(){
                var self=this;
                (function() {
                    self.arrowLeft.addEventListener("click", function() {
                        self.inner.dataset.status--;
                    });
                    self.arrowRight.addEventListener("click", function() {
                        self.inner.dataset.status++;
                    });
                    for(var i = 0, len = self.btn.length; i < len; i++) {
                        (function () {
                            var j = i;
                            /*var mutationObserver = new MutationObserver(mutationObjectCallback);
                             function mutationObjectCallback(mutationRecordsList) {
                             mutationRecordsList.forEach(function(mutationRecord) {
                             if ("attributes" === mutationRecord.type && mutationRecord.oldValue==0 && self.btn[j].dataset.active==1) {
                             console.log("image ="+j+" is in view");
                             }
                             });
                             }
                             mutationObserver.observe(self.btn[j], {attributes: true,  attributeFilter: ["data-active"], attributeOldValue: true,  childList: false});*/
                            self.btn[j].addEventListener("click", function () {
                                self.inner.dataset.status=j;
                            });
                        })();
                    }
                    var mutationObserver = new MutationObserver(mutationObjectCallback);
                    function mutationObjectCallback(mutationRecordsList) {
                        mutationRecordsList.forEach(function(mutationRecord) {
                        if ("attributes" === mutationRecord.type) {
                            self.inner.ul.style.left="-"+100*self.inner.dataset.status+"%";
                            console.log("image #"+self.inner.dataset.status+" is in the view");
                            for (var i = 0, len = self.btn.length; i < len; i++){
                                self.btn[i].dataset.active="0";
                            }
                            self.btn[self.inner.dataset.status].dataset.active="1";
                            if (self.inner.dataset.status=0) {
                                self.arrowLeft.style.display="none";
                                self.arrowRight.style.display="block";
                            }
                                else if (self.inner.dataset.status=self.btn.length-1) {
                                    self.arrowRight.style.display="none";
                                    self.arrowLeft.style.display="block";
                                }
                                    else {
                                        self.arrowRight.style.display="block";
                                        self.arrowLeft.style.display="block";
                                    }
                        }
                        });
                    }
                    mutationObserver.observe(self.inner, {attributes: true,  attributeFilter: ["data-status"], attributeOldValue: true,  childList: false});
                })();
            }/*binding events*/

            SlideShow.prototype.stop = function() {
                clearInterval(this.interval);
                /*delete hover events*/
            }/*stop continual animation*/

            SlideShow.prototype.start =  function() {
                clearInterval(this.interval);
                var self=this;
                for(var i = 0, len = self.slides.length; i < len; i++) {
                    (function() {
                        self.slides[i].addEventListener("mouseover", function() {
                            clearInterval(self.interval);
                        });
                        self.slides[i].addEventListener("mouseout", function() {
                            self.start();
                        });
                    })();
                }
                this.interval=setInterval(function() { self.animate.call(self) }, self.delay);
            }/*start continual animation*/

            SlideShow.prototype.animate = function() {
                if (this.img.length<=1) return;
                if (this.inner.dataset.status<this.img.length-1) this.inner.dataset.status++;
                else this.inner.dataset.status=0;
            }/*next image*/

